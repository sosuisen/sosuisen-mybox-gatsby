{
  "name": "sosuisen-mybox-gatsby",
  "version": "1.0.0",
  "private": true,
  "description": "My Gatsby Site",
  "author": "sosuisen",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "try-first": "git submodule update --init",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "mkdir -p ~/.ssh && echo -e '${SSH_KEY//_/\\n}' > ~/.ssh/id_rsa && chmod og-rwx ~/.ssh/id_rsa && git submodule update --remote && gatsby build",
    "build-local": "git submodule update --remote && gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "gatsby": "^2.32.3",
    "gatsby-source-filesystem": "^2.11.0",
    "gatsby-transformer-json": "^2.11.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^4.2.0"
  }
}
